{
    "workbench.colorCustomizations": {
        "activityBar.background": "#4042bf",
        "activityBar.activeBackground": "#5c40bf",
        "activityBar.activeBorder": "#c78f58",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#c78f58",
        "activityBarBadge.foreground": "#15202b",
        "titleBar.activeBackground": "#463399",
        "titleBar.inactiveBackground": "#42339999",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveForeground": "#e7e7e799",
        "statusBar.background": "#423399",
        "statusBarItem.hoverBackground": "#4d40bf",
        "statusBar.foreground": "#e7e7e7"
    },
    "peacock.color": "#639",
    "files.exclude": {
        "**/.git": true,
        "**/.gradle": true,
        "**/.idea": true,
        "**/.mvn": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/build": true,
        "**/dist": true,
        "**/target": true,
        "**/tmp": true,
        "**/.settings": true,
    },
    "search.exclude": {
        "**/node": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/build": true,
        "**/dist": true,
        "**/target": true,
        "**/tmp": true,
        "**/.settings": true,
    },
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true,
      "**/.gradle": true,
      "**/build": true,
      "**/target": true,
      "**/tmp": true,
      "**/.settings": true,
    },
    "java.configuration.updateBuildConfiguration": "automatic",
    "explorer.compactFolders": false,
    "files.autoSave": "onWindowChange"
}
